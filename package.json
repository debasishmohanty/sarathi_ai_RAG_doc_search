{
  "name": "langchain-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "setup": "ts-node setup-env.ts",
    "start": "npm run setup && ts-node src/chat.ts",
    "agent": "npm run setup && ts-node src/agent.ts",
