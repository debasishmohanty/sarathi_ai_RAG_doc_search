    "dev": "concurrently \"npm run server\" \"npm run frontend\""
  },
  "dependencies": {
    "@langchain/core": "^1.1.16",
    "@langchain/openai": "^1.2.3",
    "@langchain/community": "^0.2.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "langchain": "^1.2.11",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "pdf-parse": "^1.1.4",
    "ml-distance": "^4.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.10",
    "@types/node-fetch": "^2.6.13",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
